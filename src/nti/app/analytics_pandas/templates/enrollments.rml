<metal:block
	xmlns:tal="http://xml.zope.org/namespaces/tal"
	xmlns:metal="http://xml.zope.org/namespaces/metal"
	tal:define="options view/options">

	<tal:block tal:condition="python: options['has_enrollment_event_data']">
		<para style="normal" spaceAfter=".5in" >
			This document presents an overview of enrollment related events in <u>${options/course_names}</u> from <strong>${context/start_date}</strong> through <strong>${context/end_date}</strong>.
		</para>
	</tal:block>	
	<hr width="100%" />

	<tal:block tal:condition="python: options['has_course_catalog_view_data']">
		<setNextTemplate name="main"/>
		<nextPage />
		<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
			Overview of Course Catalog Views
		</h1>
		<hr width="100%" />
		<tal:block tal:define="course_catalog_views options/data/course_catalog_views">
			<illustration width="5in" height="3in"
					  	  tal:define="event course_catalog_views/event_course_catalog_views;
					  	  			  user_event course_catalog_views/ratio_course_catalog_views;
								      ratio_event course_catalog_views/user_course_catalog_views;
								      additional_plot course_catalog_views/average_time_length"
					      metal:use-macro="macro:four_plots_event">
			</illustration>
		</tal:block>

		<tal:block tal:condition="python: options['has_course_catalog_views_per_device_types']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Overview of Course Catalog Views
			</h1>
			<hr width="100%" />
			<tal:block tal:define="course_catalog_views options/data/course_catalog_views_per_device_types">
				<illustration width="5in" height="3in"
						  	  tal:define="event course_catalog_views/event_course_catalog_views_per_device_types;
						  	  			  user_event course_catalog_views/ratio_course_catalog_views_per_device_types;
									      ratio_event course_catalog_views/user_course_catalog_views_per_device_types;
									      additional_plot course_catalog_views/average_time_length_per_device_types"
						      metal:use-macro="macro:four_plots_event">
				</illustration>
			</tal:block>
		</tal:block>
	</tal:block>

	<tal:block tal:condition="python: options['has_course_enrollment_data']">
		<setNextTemplate name="main"/>
		<nextPage />
		<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
			Overview of Course Enrollments
		</h1>
		<hr width="100%" />
		<tal:block tal:define="course_enrollments options/data/course_enrollments">
			<illustration width="5in" height="3in"
					  	  tal:define="event course_enrollments/course_enrollments"
					      metal:use-macro="macro:one_plot_event">
			</illustration>
		</tal:block>

		<tal:block tal:condition="python: options['has_course_enrollments_per_types']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Course Enrollments per Application and Enrollment Types
			</h1>
			<hr width="100%" />
			<tal:block tal:define="course_enrollments options/data/course_enrollments_per_types">
				<illustration width="5in" height="3in"
						  	  tal:define="first_plot course_enrollments/course_enrollments_per_device_types;
						  	  			  second_plot course_enrollments/course_enrollments_per_enrollment_types"
						      metal:use-macro="macro:two_plots_event">
				</illustration>
			</tal:block>
		</tal:block>	
	</tal:block>

	<tal:block tal:condition="python: options['has_course_drop_data']">
		<setNextTemplate name="main"/>
		<nextPage />
		<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
			Overview of Course Drops
		</h1>
		<hr width="100%" />
		<tal:block tal:define="course_drops options/data/course_drops">
			<illustration width="5in" height="3in"
					  	  tal:define="event course_drops/course_drops"
					      metal:use-macro="macro:one_plot_event">
			</illustration>
		</tal:block>

		<tal:block tal:condition="python: options['has_course_drops_per_device_types']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Course Drops per Application Types
			</h1>
			<hr width="100%" />
			<tal:block tal:define="course_drops options/data/course_drops_per_device_types">
				<illustration width="5in" height="3in"
						  	  tal:define="event course_drops/course_drops_per_device_types"
						      metal:use-macro="macro:one_plot_event">
				</illustration>
			</tal:block>
		</tal:block>

		<tal:block tal:condition="python: options['has_course_drops_per_enrollment_types']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Course Drops per Enrollment Types
			</h1>
			<hr width="100%" />
			<tal:block tal:define="course_drops options/data/course_drops_per_enrollment_types">
				<illustration width="5in" height="3in"
						  	  tal:define="event course_drops/course_drops_per_enrollment_types"
						      metal:use-macro="macro:one_plot_event">
				</illustration>
			</tal:block>
		</tal:block>
	</tal:block>

	<tal:block tal:condition="python: options['has_enrollment_event_data']">
		<tal:block tal:condition="python: options['has_course_enrollments_vs_catalog_views']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Course Enrollments vs Catalog Views
			</h1>
			<hr width="100%" />
			<tal:block tal:define="enrollment_event options/data/course_enrollments_vs_catalog_views">
				<illustration width="5in" height="3in"
						  	  tal:define="event enrollment_event/course_enrollments_vs_catalog_views"
						      metal:use-macro="macro:one_plot_event">
				</illustration>
			</tal:block>
		</tal:block>

		<tal:block tal:condition="python: options['has_course_enrollments_vs_drops']">
			<setNextTemplate name="main"/>
			<nextPage />
			<h1 style="h1" fontSize="14" spaceBefore=".3in" spaceAfter=".3in">
				Course Enrollments vs Drops
			</h1>
			<hr width="100%" />
			<tal:block tal:define="enrollment_event options/data/course_enrollments_vs_drops">
				<illustration width="5in" height="3in"
						  	  tal:define="event enrollment_event/course_enrollments_vs_drops"
						      metal:use-macro="macro:one_plot_event">
				</illustration>
			</tal:block>
		</tal:block>
	</tal:block>

</metal:block>